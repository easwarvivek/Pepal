CC = gcc
CFLAGS = -O2 -DMINGW -mno-cygwin -I/usr/local/mingw
LDFLAGS = -mno-cygwin -L/usr/local/mingw
LIBS = -lgetopt -lm -liberty
PGMLIBS = -lpgm -lpbm -lgetopt
